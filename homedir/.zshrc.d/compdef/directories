# NOTE: order preference
zstyle ':completion:*:complete:(cd|pushd):*' tag-order 'local-directories directory-stack path-directories'
# NOTE: use same colors as ls when possible
zstyle ':completion:*:*:(cd|pushd):*' list-colors ${(s.:.)LS_COLORS}

